msgid "The Battle for Wesnoth 1.16"
msgstr ""

#. type: Content of: <html><body><div><div>
#: template.html:27
msgid ""
"<a id=\"bigshield\" href=\"/\" aria-label=\"Wesnoth logo\"><span "
"aria-hidden=\"true\"></span></a>"
